<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="HomeDepot WebAutomation Test" parallel="false">

    <parameter name="cloud" value="false"/>
    <parameter name="envName" value="browserstack"/>
    <parameter name="os" value="OS X"/>
    <parameter name="os_version" value="Mojave"/>
    <parameter name="browser" value="chrome"/>
    <parameter name="browserVersion" value="74.0"/>
    <parameter name="url" value="https://www.homedepot.com"/>
    <parameter name="platform" value="Mac"/>

    <test name= "HomeDepot Test">
        <classes>

            <class name="testhomepage.TestHomePage"/>
            <class name="testloginpage.TestLoginPage"/>

        </classes>
    </test>
</suite>

